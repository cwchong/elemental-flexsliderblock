{
  "name": "cwchong/elemental-flexsliderblock",
  "description": "A flexslider block for the silverstripe-elemental module",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "elemental",
    "content blocks",
    "flexslider"
  ],
  "license": "BSD-3-Clause",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "silverstripe/vendor-plugin": "^1.0",
    "dnadesign/silverstripe-elemental": "^4@dev",
    "silverstripe/cms": "^4.3",
    "silverstripe/assets": "^1.3",
    "gorriecoe/silverstripe-link": "^1.2",
    "gorriecoe/silverstripe-linkfield": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    },
    "expose": [
      "client/dist"
    ]
  },
  "autoload": {
    "psr-4": {
      "Cwchong\\ElementalFlexsliderBlock\\": "src/",
      "Cwchong\\ElementalFlexsliderBlock\\Tests\\": "tests/"
    }
  }
}